{
    "name": "formio-submission-data-validator",
    "version": "0.0.3",
    "description": "A form validation service for Formio forms",
    "license": "OSL-3.0",
    "packageManager": "pnpm@9.15.3",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "commonjs",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "lib",
        "index.ts"
    ],
    "scripts": {
        "build:lib": "esbuild './src/**/*.js' --outdir=lib --platform=node --format=cjs --packages=external",
        "build:dist": "esbuild src/index.js --outdir=dist --bundle --platform=node --format=cjs --packages=external && cp src/index.d.ts ./dist/",
        "build": "pnpm run build:lib && pnpm run build:dist",
        "lint": "eslint .",
        "prepare": "pnpm run build"
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "dependencies": {
        "@formio/core": "v2.3.0-patch.2",
        "@formio/node-fetch-http-proxy": "^1.1.0",
        "@formio/vm": "0.2.8-patch.2",
        "esbuild": "^0.27.0",
        "fast-json-patch": "^3.1.1",
        "formiojs": "4.21.5",
        "inputmask": "^5.0.8",
        "lodash": "^4.17.21",
        "moment": "^2.29.3",
        "moment-timezone": "^0.5.34",
        "nunjucks": "^3.2.4",
        "resolve": "^1.22.10"
    },
    "devDependencies": {
        "@changesets/cli": "^2.29.7",
        "eslint": "^9.22.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/myqld/formio-submission-data-validator.git"
    },
    "homepage": "https://github.com/myqld/formio-submission-data-validator#readme"
}
